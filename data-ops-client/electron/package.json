{
  "name": "data-ops",
  "private": true,
  "license": "MIT",
  "author": "yuqin, hexi",
  "version": "0.0.3",
  "main": "main.js",
  "homepage": "./",
  "keywords": [
    "electron"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba/data-ops"
  },
  "description": "Electron Application",
  "scripts": {
    "electron:build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^20.1.4",
    "electron-builder": "^23.3.3",
    "electron-is-dev": "^2.0.0"
  },
  "icon": "logo/logo.png",
  "build": {
    "appId": "com.data-opt",
    "directories": {
      "output": "../electron-build"
    },
    "asar": false,
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "app/**/*",
      "jre/**/*",
      "my.bat"
    ],
    "mac": {
      "icon": "logo/logo.png",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  }
}
